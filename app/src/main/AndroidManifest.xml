<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medhigh.meetmd">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".search.SearchDoctor">

        </activity>
        <activity android:name=".login.MainActivity">

        </activity>
        <activity
            android:name=".login.NewPasswordActivity"
            android:label="@string/set_new_pass"/>
        <activity
            android:name=".appointments.ApointmentsActivity"
            android:theme="@style/AppTheme"
            android:label="@string/appointments_title">

        </activity>
        <activity android:name=".appointments.AppointmentDetailsActivity"
            android:theme="@style/AppTheme"
            android:label="@string/appointments">

        </activity>
        <activity android:name=".survey.SurveyActivity"
            android:theme="@style/AppTheme"
            android:label="@string/surveys">

        </activity>
        <activity android:name=".information.InformationActivity"
            android:theme="@style/AppTheme"
            android:label="@string/information">

        </activity>
        <activity android:name=".timeChooseFragment.DoctorChooseTimeActivity"
            android:theme="@style/AppTheme">

        </activity>
        <activity android:name=".appointments.AppointmentConfirmActivity"/>
        <activity android:name=".survey.SurveyRatingActivity"/>
        <activity android:name=".news.NewsActivity">

        </activity>
        <activity android:name=".trashTest.Test">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
